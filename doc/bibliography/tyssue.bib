  file = {Attachment:/home/guillaume/.zotero/zotero/hjbatadb.default/zotero/storage/WERPFI8I/Vasquez, Tworoger, Martin - 2014 - Dynamic myosin phosphorylation regulates contractile pulses and tissue integrity during epithelial mo.pdf:application/pdf},
  eprinttype = {pmid},
  eprint = {25092658}
}

@article{mason_tuning_2011,
